<?xml version="1.0" encoding="utf-8"?>
<library xmlns="duell">

    <ndll name="inputios" build-file-path="project/Build.xml" bin-path="ndll" />

    <haxe-compile-arg value="--macro keep('de.polygonal.ds.pooling.DynamicObjectPool')" />
